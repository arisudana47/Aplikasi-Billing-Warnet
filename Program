    private void tb_1MouseClicked(java.awt.event.MouseEvent evt) {                                  
        // TODO add your handling code here:
        idu.setText(tabModel.getValueAt(tb_1.getSelectedRow(),0) + "");
        user.setText(tabModel.getValueAt(tb_1.getSelectedRow(), 1) + "");
        id.setText(tabModel.getValueAt(tb_1.getSelectedRow(), 2) + "");
        waktu.setText(tabModel.getValueAt(tb_1.getSelectedRow(), 3) + "");
        tambahan.setSelectedItem(tabModel.getValueAt(tb_1.getSelectedRow(), 4) + "");
        harga.setText(tabModel.getValueAt(tb_1.getSelectedRow(), 5)+ "");
        w_habis.setText(tabModel.getValueAt(tb_1.getSelectedRow(), 8)+ "");
    }                                 

    private void updateActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
        
            int a, b, c;
            if(tambahan.getSelectedItem().equals("30 menit")){
                a = 2500;
                b = Integer.parseInt(harga.getText());
                c = a + b;
                harga.setText("" + c);
            }else if(tambahan.getSelectedItem().equals("1 jam")){
                a = 5000;
                b = Integer.parseInt(harga.getText());
                c = a + b;
                harga.setText("" + c);
            }else if(tambahan.getSelectedItem().equals("2 jam")){
                a = 10000;
                b = Integer.parseInt(harga.getText());
                c = a + b;
                harga.setText("" + c);
            }else if(tambahan.getSelectedItem().equals("3 jam")){
                a = 15000;
                b = Integer.parseInt(harga.getText());
                c = a + b;
                harga.setText("" + c);
            }else if(tambahan.getSelectedItem().equals("4 jam")){
                a = 20000;
                b = Integer.parseInt(harga.getText());
                c = a + b;
                harga.setText("" + c);
            }
         try{
             
            st = on.createStatement();
            st.executeUpdate("update tb_pesan set user='" + user.getText() + "', waktu = '" +
                    waktu.getText() + "', waktu_tambahan = '" +
                    tambahan.getSelectedItem() + "', pc = '" +
                    id.getText()+ "', harga = '" +
                    harga.getText() + "' where id='" + tabModel.getValueAt(tb_1.getSelectedRow(), 0) + "'");
            tampilData();
            
            JOptionPane.showMessageDialog(null, "Data Berhasil Diupdate");
            user.setText("");
            waktu.setText("");
            id.setText("");
            harga.setText("");
            tambahan.setSelectedItem("null");
        } catch (Exception e){
            e.printStackTrace();
        }
    }                                      

    private void deleteActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
        try {
            int jawab;
            if ((jawab = JOptionPane.showConfirmDialog(null, "Hapus?", "Komfirmasi", JOptionPane.YES_NO_CANCEL_OPTION)) == 0){
                st = on.createStatement();
                st.executeUpdate("delete from tb_pesan where " + "id='" + tabModel.getValueAt(tb_1.getSelectedRow(), 0) + "'");
                tampilData();
                user.setText("");
                waktu.setText("");
                id.setText("");
                harga.setText("");
            }
        } catch (Exception e){
            e.printStackTrace();
        }
    }                                      

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        try{
            st = on.createStatement();
            tabModel.getDataVector().removeAllElements();
            tabModel.fireTableDataChanged();
            rs = st.executeQuery("select * from tb_pesan where id like '%" + search.getText() + "%' or user like'%" +
                    search.getText() + "%' or waktu like'%" + search.getText() + "%' or pc like'%" + search.getText() + "%'" +
                    "or harga like'%" + search.getText() + "%' or tanggal like'%" +
                    search.getText() + "%' or jam_main like'%" +
                    search.getText() + "%'");
            while(rs.next()){
                Object[] data = {
                    rs.getString("id"),
                    rs.getString("user"),
                    rs.getString("pc"),
                    rs.getString("waktu"),
                    rs.getString("waktu_tambahan"),
                    rs.getString("harga"),
                    rs.getString("tanggal"),
                    rs.getString("jam_main"),
                    rs.getString("jam_habis")
                };
                tabModel.addRow(data);
            }   
        } catch (Exception e){
            e.printStackTrace();
        }
    }                                        

    private void tambahanActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
    }                                        

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
