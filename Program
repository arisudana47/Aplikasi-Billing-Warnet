    private void regisActionPerformed(java.awt.event.ActionEvent evt) {                                      
        // TODO add your handling code here:
        String jenis = null;
        String uname = nama.getText();
        String uuser = user.getText();
        String upass = pass.getText();
        if(l.isSelected()){
            jenis = "L";
        }else if(p.isSelected()){
            jenis = "P";
        }
        if (uname.equals("")) {
        JOptionPane.showMessageDialog(null,"lengkapi data !!");  
        }
        else if(uuser.equals("")){
        JOptionPane.showMessageDialog(null,"lengkapi data !!"); 
        }
        else if(upass.equals("")){
        JOptionPane.showMessageDialog(null,"lengkapi data !!");   
        }
        else{
        try{
            st = on.createStatement();
            st.executeUpdate("INSERT INTO tb_member set " + "nama_member='" + nama.getText() + "'," + "jk='" + 
            jenis + "'," + "umur='" + umur.getSelectedItem() + "'," + "username='" + user.getText() + "'," + "password='" + pass.getText()+ "'");
            JOptionPane.showMessageDialog(null,"Berhasil Disimpan");
            nama.setText("");
            umur.setSelectedItem("");
            pass.setText("");
            user.setText("");
            buttonGroup1.clearSelection();
            loginmember li = new loginmember();
            li.setVisible(true);
            dispose();
        } catch (Exception e){
            e.printStackTrace();
        }
        }
        
    }                                     

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        loginmember li = new loginmember();
        li.setVisible(true);
        dispose();
    }                                        

    private void namaKeyTyped(java.awt.event.KeyEvent evt) {                              
        // TODO add your handling code here:
            if (nama.getText().length()==50){
            evt.consume();
            JOptionPane.showMessageDialog(null, "Maksimal 50 Karakter","Peringatan",JOptionPane.WARNING_MESSAGE);
        }
    }                             

    private void userKeyTyped(java.awt.event.KeyEvent evt) {                              
        // TODO add your handling code here:
            if (user.getText().length()==10){
            evt.consume();
            JOptionPane.showMessageDialog(null, "Maksimal 10 Karakter","Peringatan",JOptionPane.WARNING_MESSAGE);
        }  
    }                             

    private void passKeyTyped(java.awt.event.KeyEvent evt) {                              
        // TODO add your handling code here:
            if (pass.getText().length()==8){
            evt.consume();
            JOptionPane.showMessageDialog(null, "Maksimal 8 Karakter","Peringatan",JOptionPane.WARNING_MESSAGE);
        }  
    }                             

