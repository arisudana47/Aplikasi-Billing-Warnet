    private void hargaActionPerformed(java.awt.event.ActionEvent evt) {                                      
        // TODO add your handling code here:
    }                                     

    private void btn_pesanActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        int detik1 = 0 ;
        int menit1 = 0 ;
        String waktu = null;
        if(jRadioButton1.isSelected()){
            waktu = "1 jam";
        }else if(jRadioButton2.isSelected()){
            waktu = "2 jam";
        }else if(jRadioButton3.isSelected()){
            waktu = "3 jam";
        }else if(jRadioButton4.isSelected()){
            waktu = "4 jam";
        }
        if (user.getText().equals("") | harga.getText().equals("") | komputer.getSelectedItem().equals("Pilih komputer")) {
          JOptionPane.showMessageDialog(null,"lengkapi data !!");  
        }else{
        try{
            st = on.createStatement();
            st.executeUpdate("insert into tb_pesan set "+ "id='" + id.getText() + "', " + "user='" + user.getText() + "', " + "waktu='" + 
           waktu + "'," + "waktu_tambahan='" + null + "'," + "pc='" + komputer.getSelectedItem() + "'," + "harga='" + harga.getText() + "'," + "tanggal='" + tgl.getText() + "'," + "jam_main='" + lblwktu.getText() + "'," + "jam_habis='" + whabis.getText() + "'");
            JOptionPane.showMessageDialog(null, "Berhasil Memesan");
            menuadmin ma = new menuadmin();
        ma.setVisible(true);
        dispose();
            user.setText("");
            buttonGroup1.clearSelection();
            komputer.setSelectedItem("Pilih komputer");
            harga.setText("");
        } catch (Exception e){
            e.printStackTrace();
        }
        }
    }                                         

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        menuadmin ma = new menuadmin();
        ma.setVisible(true);
        dispose();
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        user.setText("");
            buttonGroup1.clearSelection();
            komputer.setSelectedItem("Pilih komputer");
            harga.setText("");
    }                                        

    private void jRadioButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                              
        // TODO add your handling code here:
        if (this.jRadioButton4.isSelected()){
            this.harga.setText("20000");
        }
    }                                             

