    }                                        

    private void update1ActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
                    int a, b, c;
            if(tambahan.getSelectedItem().equals("30 menit")){
                a = 1500;
                b = Integer.parseInt(harga.getText());
                c = a + b;
                harga.setText("" + c);
            }else if(tambahan.getSelectedItem().equals("1 jam")){
                a = 3000;
                b = Integer.parseInt(harga.getText());
                c = a + b;
                harga.setText("" + c);
            }else if(tambahan.getSelectedItem().equals("2 jam")){
                a = 8000;
                b = Integer.parseInt(harga.getText());
                c = a + b;
                harga.setText("" + c);
            }else if(tambahan.getSelectedItem().equals("3 jam")){
                a = 13000;
                b = Integer.parseInt(harga.getText());
                c = a + b;
                harga.setText("" + c);
            }else if(tambahan.getSelectedItem().equals("4 jam")){
                a = 18000;
                b = Integer.parseInt(harga.getText());
                c = a + b;
                harga.setText("" + c);
            }
         try{
             
            st = on.createStatement();
            st.executeUpdate("update tb_pesan set user='" + user.getText() + "', waktu = '" +
                    waktu.getText() + "', waktu_tambahan = '" +
                    tambahan.getSelectedItem() + "', pc = '" +
                    id.getText()+ "', harga = '" +
                    harga.getText() + "' where id='" + tabModel.getValueAt(tb_1.getSelectedRow(), 0) + "'");
            tampilData();
            
            JOptionPane.showMessageDialog(null, "Data Berhasil Diupdate");
            user.setText("");
            waktu.setText("");
            id.setText("");
            harga.setText("");
            tambahan.setSelectedItem("null");
        } catch (Exception e){
            e.printStackTrace();
        }
    }                                       

