    java.util.Date tglsekarang = new java.util.Date();
private SimpleDateFormat smpdtfmt = new SimpleDateFormat("yyyy-MM-dd", Locale.getDefault());
//diatas adalah pengaturan format penulisan, bisa diubah sesuai keinginan.
private String tanggal = smpdtfmt.format(tglsekarang);
    
    /**
     * Creates new form 
     */
    
    public menuuser() {
        initComponents();
        harga.setEnabled(false);
        whabis.setEnabled(false);
        setJam();
        tgl.setText(tanggal);
        id.setEnabled(false);
        kode();
    }
    public final void setJam(){
    ActionListener taskPerformer = new ActionListener() {
    public void actionPerformed(ActionEvent evt) {
    String nol_jam = "", nol_menit = "",nol_detik = "";
    int jam1 = 0 ;
        int detik1 = 0 ;
        int menit1 = 0 ;
        String waktu = null;
        if(jRadioButton1.isSelected()){
            jam1 =  01 ;
            menit1 = 00;
            detik1 = 00;
            waktu = "01:00:00";
        }else if(jRadioButton2.isSelected()){
            jam1 =  02 ;
            menit1 = 00;
            detik1 = 00;
            waktu = "02:00:00";
        }else if(jRadioButton3.isSelected()){
            jam1 =  03 ;
            menit1 = 00;
            detik1 = 00;
            waktu = "03:00:00";
        }else if(jRadioButton4.isSelected()){
            jam1 =  04 ;
            menit1 = 00;
            detik1 = 00;
            waktu = "04:00:00";
        }

    java.util.Date dateTime = new java.util.Date();
    int nilai_jam = dateTime.getHours();
    int nilai_menit = dateTime.getMinutes();
    int nilai_detik = dateTime.getSeconds();

    if(nilai_jam <= 9) nol_jam= "0";
    if(nilai_menit <= 9) nol_menit= "0";
    if(nilai_detik <= 9) nol_detik= "0";

    String jam = nol_jam + Integer.toString(nilai_jam);
    String menit = nol_menit + Integer.toString(nilai_menit);
    String detik = nol_detik + Integer.toString(nilai_detik);
    
    Integer jam2 = Integer.parseInt(jam) + jam1;
    Integer menit2 = Integer.parseInt(menit) + menit1;
    Integer detik2 = Integer.parseInt(detik) + detik1;
    
    String jam3 = Integer.toString(jam2);
    String menit3 = Integer.toString(menit2);
    String detik3 = Integer.toString(detik2);
    
    lblwktu.setText(jam+":"+menit+":"+detik+"");
    whabis.setText(jam2+":"+menit2+":"+detik2+"");
    }
    };
new Timer(1000, taskPerformer).start();
}
    public void kode(){
        try{
            st = on.createStatement();
            rs = st.executeQuery("select * from tb_pesan ORDER By id DESC");
            if(rs.next()){
            String kd_ot = rs.getString("id").substring(2);
            String an = ""+(Integer.parseInt(kd_ot)+1);
            String nol = "";
            if(an.length() == 0){
                nol = "000";
            }else if(an.length() == 1){
                nol = "00";
            }else if(an.length() == 2){
                nol = "0";
            }else if(an.length() == 3){
                nol = "";
            }
                id.setText("PL" + nol + an);
            }else{
                id.setText("PL001");
            
        }
        }catch (Exception e){
               e.printStackTrace();
        }
    }


