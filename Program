    private void btnloginActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
       try{
           String password;
           password = pass.getText();
           String username;
           username = user.getText();
           
           st = on.createStatement();
           rs = st.executeQuery("select * from tb_admin "
               + "where username= "
               + "'"+ username +"'" + "and password='"+ password +"'");
           int baris = 0;
           while (rs.next()){
               baris = rs.getRow();
           }
           if (baris == 1){
               JOptionPane.showMessageDialog(null, "Login berhasil");
               menuUtama ma = new menuUtama();
               ma.setVisible(true);
               dispose();
           }
           else{
               JOptionPane.showMessageDialog(null, "Login gagal");
           }
       }catch (Exception e){
           JOptionPane.showMessageDialog(null, e.getMessage());
       }
    }                                        

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        registeradmin ra = new registeradmin();
        ra.setVisible(true);
        dispose();
    }                                        

    private void userKeyTyped(java.awt.event.KeyEvent evt) {                              
        // TODO add your handling code here:
            if (user.getText().length()==10){
            evt.consume();
            JOptionPane.showMessageDialog(null, "Maksimal 10 Karakter","Peringatan",JOptionPane.WARNING_MESSAGE);
        }
    }                             

    private void passKeyTyped(java.awt.event.KeyEvent evt) {                              
        // TODO add your handling code here:
            if (pass.getText().length()==8){
            evt.consume();
            JOptionPane.showMessageDialog(null, "Maksimal 8 Karakter","Peringatan",JOptionPane.WARNING_MESSAGE);
        }
    }                             

